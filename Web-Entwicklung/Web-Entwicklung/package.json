{
  "name": "web-entwicklung",
  "version": "0.0.0",
  "description": "Web-Entwicklung",
  "author": {
    "name": "Kai Kegeler, Lena Arnolds"
  },
  "devDependencies": {
    "babili": "latest",
    "browserify": "latest",
    "eslint": "latest",
    "less": "latest"
  },
  "dependencies": {
    "express": "latest",
    "googlemaps": "latest",
    "socket.io": "latest"
  },
  "scripts": {
    "lint": "eslint ./server.js && eslint ./public/client.js",
    "browserify": "browserify ./public/client.js > ./public/clientB.js",
    "minify": "babili ./public/clientB.js > ./public/main.js",
    "lessc": "lessc --clean-css ./public/style.less ./public/style.min.css",
    "debug": "npm run lint && npm run browserify",
    "build": "npm run lint && npm run browserify && npm run minify && npm run lessc",
    "start": "node start ./server.js",
    "clean": ""
  }
}
